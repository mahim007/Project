<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="comm.example.RegisterBean" table="lib_members">
  
      <id name="mid" column="mid">
          <generator class="assigned"/>
      </id>
      <property name="mpass" column="mpass"/>
      <property name="mname" column="mname"/>
      <property name="maddress" column="maddress"/>
      <property name="mgender" column="mgender"/>
      <property name="memail" column="memail"/>
      <property name="mcontact" column="mcontact"/>
      <property name="moccupation" column="moccupation"/>
  </class>
</hibernate-mapping>
